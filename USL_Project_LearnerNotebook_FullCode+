    "    # creating a copy of the original data\n",
    "    df1 = df.copy()\n",
    "\n",
    "    # adding kmeans cluster labels to the original and scaled dataframes\n",
    "    k_means_df[\"KM_segments\"] = kmeans.labels_\n",
    "    df1[\"KM_segments\"] = kmeans.labels_\n",
